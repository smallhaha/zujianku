<transferCode>
    <codeFragment name="basic" id="10000">
        <code name="Html">
            <a-layout-sider hide-trigger collapsible :collapsed="collapsed">
                <div class="collapse_main">
                  <div @click="onCollapse">
                    <div class="collapse_right" v-if="collapsed">
                      <IconLeft />
                    </div>
                    <div class="collapse" v-else>
                      <IconRight />
                    </div>
                  </div>
                </div>
                <a-anchor :style="{ width: '200px' }">
                  <a-anchor-link
                    v-for="item in pageList.child"
                    :key="item.herf"
                    :href="'#' + item.herf"
                    >{{ item.label }}</a-anchor-link
                  >
                </a-anchor>
              </a-layout-sider>
        </code>
        <code name="CSS">
            .HomeLay_demo :deep(.arco-layout-content) {
                overflow-y: auto;
              }
              .HomeLay_demo .HomeLay_demo_left {
                width: 240px;
              }
              .HomeLay_demo .HomeLay_demo_left :deep(.arco-layout-sider-light) {
                border-right: 1px solid #dbdee2;
                box-shadow: none;
                position: fixed;
                left: 0px;
                top: 80px;
              }
            </code>
        <code name='JS'>
            var propId = window.location.search.split('=')[1]
            $('#' + propId).show().siblings('.transfer').hide()
        </code>
    </codeFragment>
